<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorLIGHTCYAN"
        tools:context=".HomeActivity">

    <View
            android:layout_width="361dp"
            android:layout_height="161dp"
            android:id="@+id/view" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.8" android:layout_marginTop="16dp"/>
    <View
            android:layout_width="361dp"
            android:layout_height="58dp"
            android:id="@+id/view3" android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/view"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.571"/>
    <View
            android:layout_width="361dp"
            android:layout_height="256dp"
            android:id="@+id/view4" android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/view3"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" app:layout_constraintHorizontal_bias="0.571"
            app:layout_constraintVertical_bias="0.204"/>
    <ImageView
            android:layout_width="79dp"
            android:layout_height="77dp" app:srcCompat="?android:attr/alertDialogIcon"
            android:id="@+id/imageView3" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/view"
            app:layout_constraintStart_toStartOf="@+id/view" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/view3" app:layout_constraintVertical_bias="0.202"/>
    <TextView
            android:layout_width="214dp"
            android:layout_height="28dp"
            android:id="@+id/viewUsername" app:layout_constraintStart_toEndOf="@+id/imageView3"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/view" android:layout_marginTop="28dp"
            app:layout_constraintEnd_toEndOf="@+id/view" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.592"
            android:text="Name" android:visibility="visible"/>
    <TextView
            android:text="A/C No"
            android:layout_width="69dp"
            android:layout_height="28dp"
            android:id="@+id/textView13" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/viewUsername" app:layout_constraintStart_toEndOf="@+id/imageView3"
            android:layout_marginLeft="32dp" android:layout_marginStart="32dp"
            app:layout_constraintBottom_toBottomOf="@+id/view" android:layout_marginBottom="8dp"
            app:layout_constraintVertical_bias="0.295"/>
    <TextView
            android:text="TextView"
            android:layout_width="136dp"
            android:layout_height="28dp"
            android:id="@+id/textView14" app:layout_constraintStart_toEndOf="@+id/textView13"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/viewUsername" app:layout_constraintBottom_toBottomOf="@+id/view"
            android:layout_marginBottom="8dp" app:layout_constraintEnd_toEndOf="@+id/view"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.08"
            app:layout_constraintVertical_bias="0.295"/>
    <TextView
            android:text="A/C Balance"
            android:layout_width="88dp"
            android:layout_height="28dp"
            tools:layout_editor_absoluteX="64dp" android:id="@+id/textView16" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/view3" app:layout_constraintBottom_toBottomOf="@+id/view3"
            android:layout_marginBottom="8dp" app:layout_constraintVertical_bias="0.642"/>
    <TextView
            android:text="TextView"
            android:layout_width="198dp"
            android:layout_height="28dp"
            android:id="@+id/textView17" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/view3" app:layout_constraintStart_toEndOf="@+id/textView16"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/view3" android:layout_marginBottom="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintVertical_bias="0.631"/>
    <Button
            android:text="Transection"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button5" android:layout_marginTop="44dp"
            app:layout_constraintTop_toTopOf="@+id/view4" app:layout_constraintStart_toStartOf="@+id/view4"
            android:layout_marginLeft="60dp" android:layout_marginStart="60dp"/>
    <Button
            android:text="Statement"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/button6" app:layout_constraintStart_toEndOf="@+id/button5" android:layout_marginLeft="48dp"
            android:layout_marginStart="48dp" app:layout_constraintTop_toTopOf="@+id/view4"
            android:layout_marginTop="44dp" app:layout_constraintEnd_toEndOf="@+id/view4" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.285"/>
    <Button
            android:text="Add Card"
            android:layout_width="119dp"
            android:layout_height="wrap_content"
            android:id="@+id/button8" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/view4" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/button5" app:layout_constraintStart_toStartOf="@+id/view4"
            android:layout_marginLeft="60dp" android:layout_marginStart="60dp"
            app:layout_constraintVertical_bias="0.54"/>
    <Button
            android:text="Settings"
            android:layout_width="103dp"
            android:layout_height="wrap_content"
            android:id="@+id/button9"
            android:layout_marginTop="60dp"
            app:layout_constraintTop_toBottomOf="@+id/button6" app:layout_constraintStart_toEndOf="@+id/button8"
            android:layout_marginLeft="52dp" android:layout_marginStart="52dp"/>
</android.support.constraint.ConstraintLayout>