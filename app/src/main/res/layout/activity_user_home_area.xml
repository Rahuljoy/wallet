<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorLIGHTCYAN"
        tools:context=".UserHomeAreaActivity">

    <View
            android:layout_width="361dp"
            android:layout_height="161dp"
            android:background="#B2DFDB"
            android:id="@+id/view" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.8" android:layout_marginTop="16dp"/>
    <View
            android:layout_width="361dp"
            android:layout_height="58dp"
            android:background="#A7FFEB"
            android:id="@+id/view3" android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/view"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.571"/>
    <View
            android:layout_width="361dp"
            android:layout_height="279dp"
            android:id="@+id/view4" android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/view3"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="8dp" app:layout_constraintHorizontal_bias="0.571"
            app:layout_constraintVertical_bias="0.619"/>
    <ImageView
            android:layout_width="79dp"
            android:layout_height="77dp" app:srcCompat="@drawable/ic_menu_camera"
            android:id="@+id/imageView3" android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="@+id/view"
            app:layout_constraintStart_toStartOf="@+id/view" android:layout_marginLeft="8dp"
            android:layout_marginStart="8dp" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toTopOf="@+id/view3" app:layout_constraintVertical_bias="0.202"/>
    <TextView
            android:layout_width="156dp"
            android:layout_height="28dp"
            android:id="@+id/viewUsername" app:layout_constraintStart_toEndOf="@+id/imageView3"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/view" android:layout_marginTop="72dp"
            app:layout_constraintEnd_toEndOf="@+id/view" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.156"
            android:text="Name" android:visibility="visible"/>
    <TextView
            android:text="A/C No"
            android:layout_width="56dp"
            android:layout_height="28dp"
            android:id="@+id/textView13" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/viewUsername" app:layout_constraintStart_toEndOf="@+id/imageView3"
            android:layout_marginLeft="24dp" android:layout_marginStart="24dp"
            app:layout_constraintBottom_toBottomOf="@+id/view" android:layout_marginBottom="8dp"
            app:layout_constraintVertical_bias="0.698"/>
    <TextView
            android:text="TextView"
            android:layout_width="148dp"
            android:layout_height="28dp"
            android:id="@+id/textView14" app:layout_constraintStart_toEndOf="@+id/textView13"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/viewUsername" app:layout_constraintBottom_toBottomOf="@+id/view"
            android:layout_marginBottom="8dp" app:layout_constraintEnd_toEndOf="@+id/view"
            android:layout_marginEnd="8dp" android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.172"
            app:layout_constraintVertical_bias="0.698"/>
    <TextView
            android:text="A/C Balance"
            android:layout_width="88dp"
            android:layout_height="28dp"
            tools:layout_editor_absoluteX="64dp" android:id="@+id/textView16" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/view3" app:layout_constraintBottom_toBottomOf="@+id/view3"
            android:layout_marginBottom="8dp" app:layout_constraintVertical_bias="0.642"
            tools:ignore="MissingConstraints"/>
    <TextView
            android:text="TextView"
            android:layout_width="198dp"
            android:layout_height="28dp"
            android:id="@+id/textView17" android:layout_marginTop="8dp"
            app:layout_constraintTop_toTopOf="@+id/view3" app:layout_constraintStart_toEndOf="@+id/textView16"
            android:layout_marginLeft="8dp" android:layout_marginStart="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/view3" android:layout_marginBottom="8dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
            app:layout_constraintVertical_bias="0.631"/>
    <Button
            android:text="Transaction"
            android:layout_width="99dp"
            android:layout_height="37dp"
            android:id="@+id/transactionButton" android:layout_marginTop="44dp"
            app:layout_constraintTop_toTopOf="@+id/view4" app:layout_constraintStart_toStartOf="@+id/view4"
            android:layout_marginLeft="60dp" android:layout_marginStart="60dp"
            android:background="#64FFDA"/>
    <Button
            android:text="Statement"
            android:layout_width="99dp"
            android:layout_height="37dp"
            android:id="@+id/StatementButton" app:layout_constraintStart_toEndOf="@+id/transactionButton" android:layout_marginLeft="48dp"
            android:layout_marginStart="48dp" app:layout_constraintTop_toTopOf="@+id/view4"
            android:layout_marginTop="44dp"
            android:background="#64FFDA" app:layout_constraintEnd_toEndOf="@+id/view4" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.17"/>
    <Button
            android:text="Card"
            android:layout_width="99dp"
            android:layout_height="37dp"
            android:id="@+id/cardButton" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/view4" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/transactionButton" app:layout_constraintStart_toStartOf="@+id/view4"
            android:layout_marginLeft="60dp" android:layout_marginStart="60dp"
            app:layout_constraintVertical_bias="0.358"
            android:background="#64FFDA"/>
    <Button
            android:text="Settings"
            android:layout_width="99dp"
            android:layout_height="37dp"
            android:id="@+id/settingButton"
            android:layout_marginTop="60dp"
            app:layout_constraintTop_toBottomOf="@+id/StatementButton"
            app:layout_constraintStart_toEndOf="@+id/cardButton"
            android:layout_marginLeft="60dp" android:layout_marginStart="60dp"
            android:background="#64FFDA" app:layout_constraintEnd_toEndOf="@+id/view4" android:layout_marginEnd="8dp"
            android:layout_marginRight="8dp" app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintBottom_toBottomOf="@+id/view4" android:layout_marginBottom="8dp"
            app:layout_constraintVertical_bias="0.0"/>
    <Switch
            android:text="@string/logout"
            android:layout_width="114dp"
            android:layout_height="28dp"
            android:id="@+id/logOut"
            android:textAllCaps="false"
            android:textColor="#D50000" android:textSize="17sp" android:textStyle="bold"
            tools:ignore="MissingConstraints" app:layout_constraintBottom_toBottomOf="@+id/view"
            android:layout_marginBottom="120dp" app:layout_constraintEnd_toEndOf="@+id/view"
            android:layout_marginEnd="12dp" android:layout_marginRight="12dp"/>

</android.support.constraint.ConstraintLayout>